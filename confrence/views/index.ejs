<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />
    <title>Live session</title>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3"
      crossorigin="anonymous"
    />
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
      crossorigin="anonymous"
    ></script>
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta2/css/all.min.css"
      integrity="sha512-YWzhKL2whUzgiheMoBFwW8CKV4qpHQAEuvilg9FAn5VJUDwKZZxkJNuGM4XkWuk94WCrrwslk8yWNGmY1EduTA=="
      crossorigin="anonymous"
      referrerpolicy="no-referrer"
    />
    <link rel="stylesheet" href="video.css" />

    <!-- <script type="text/javascript">
      var roomID = "<%= RoomId %>";
    </script> -->

    
    <!-- modal -->
    <link
      rel="stylesheet"
      href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css"
    />
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
    <!-- modal end-->
  </head>

  <body>
    <!--<i class="fa fa-microphone-slash" aria-hidden="true"></i>  -->
    <div class="main">
      <div class="main__letf">
        <div class="main_videos">
          <div id="video-grid"></div>
        </div>
        <div class="main__controls">
          <div class="main__controls__blocks">
            <div class="main__controls__button mutebnt" onclick="muteUnmute();">
              <i class="fa fa-microphone" aria-hidden="true"></i>
              <span> Mute </span>
            </div>
            <div
              class="main__controls__button videoctl"
              onclick="stopPlayVideo();"
            >
              <i class="fas fa-video"></i>
              <span>Stop video</span>
            </div>
            <div class="main__controls__button">
              <i class="fa fa-shield-alt"></i>
              <span> Security </span>
            </div>
            <div class="main__controls__button">
              <i class="fa fa-user-friends"></i>

              <span> Participants </span>
            </div>

            <div class="main__controls__button">
              <small></small>
              <span class="leavemeet"> Leave meeting </span>
            </div>
          </div>
        </div>
      </div>
      <div class="main__right">
        <div class="main__header">
          <h6>Chat</h6>
        </div>
        <div class="main__chat_window">
          <ul class="messge"></ul>
        </div>
        <div class="main__message_container">
          <input id="chat_message" type="text" placeholder="type messge" />
        </div>
      </div>
    </div>
    <!-- modal -->
    <div id="myModal" class="modal1 modal fade" role="dialog">
      <div class="modal-dialog">
        <!-- Modal content-->
        <div class="modal-content">
          <div class="modal-header">
            <h4 class="modal-title">Your Score</h4>
          </div>
          <div class="score">
            <p>Nice (xyz%)</p>
          </div>
          <div class="modalbtndiv">
            <button type="button" class="modalbtn" data-dismiss="modal">
              You Want to back just click here
            </button>
          </div>
        </div>
      </div>
    </div>
    <!-- modal -->
    <script type="text/javascript">
      console.log("<%=  RoomId %>");

      const roomID = "<%=  RoomId %>";
    </script>
    <script src="/socket.io/socket.io.js" defer></script>
    <script src="https://unpkg.com/peerjs@1.3.1/dist/peerjs.min.js" async></script>
    <script src="../public/video.js" defer></script>
  </body>
</html>
